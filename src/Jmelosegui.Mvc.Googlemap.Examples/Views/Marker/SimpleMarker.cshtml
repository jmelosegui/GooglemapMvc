@using Jmelosegui.Mvc.GoogleMap
@{
    ViewBag.Title = "Simple Marker";
}

@(
        Html.GoogleMap()
            .Name("map")
            .ApiKey((string)ViewData["GoogleMapApiKey"])
            .Height(300)
            .Center(c => c.Latitude(23.140971656573335)
                          .Longitude(-82.39435493946075))
            .Markers(m => m.Add().Title("Hello World!"))
)